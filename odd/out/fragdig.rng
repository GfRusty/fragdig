         <ref name="att.ranging.attribute.atLeast"/>
         <ref name="att.ranging.attribute.atMost"/>
         <ref name="att.ranging.attribute.min"/>
         <ref name="att.ranging.attribute.max"/>
         <ref name="att.ranging.attribute.confidence"/>
